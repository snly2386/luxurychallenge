<p id="notice"><%= notice %></p>

<h1>Listing Agents</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Agent mls</th>
      <th>Description</th>
      <th>Image url</th>
      <th>Office mls</th>
      <th>Phone number</th>
      <th>Country code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agents.each do |agent| %>
      <tr>
        <td><%= agent.first_name %></td>
        <td><%= agent.last_name %></td>
        <td><%= agent.email %></td>
        <td><%= agent.agent_mls %></td>
        <td><%= agent.description %></td>
        <td><%= agent.image_url %></td>
        <td><%= agent.office_mls %></td>
        <td><%= agent.phone_number %></td>
        <td><%= agent.country_code %></td>
        <td><%= link_to 'Show', agent %></td>
        <td><%= link_to 'Edit', edit_agent_path(agent) %></td>
        <td><%= link_to 'Destroy', agent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agent', new_agent_path %>
